{
  "name": "noname-node",
  "version": "1.0.0",
  "description": "NoName - node backend",
  "main": "dist/index.js",
  "author": "Guilherme Afonso Borchardt",
  "license": "MIT",
  "private": true,
  "scripts": {
    "predev": "node src/database/index",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "db": "node node_modules/db-migrate/bin/db-migrate",
    "db-new-migration": "yarn db create --sql-file",
    "db-run-migrations": "yarn db up",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "date-fns": "^2.14.0",
    "db-migrate": "^0.11.11",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pg": "^8.2.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^5.2.1",
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.3",
    "@types/pg": "^7.14.3",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/experimental-utils": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@typescript-eslint/typescript-estree": "^3.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.3"
  }
}
